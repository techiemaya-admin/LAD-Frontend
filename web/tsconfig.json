{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/features/*": [
        "./src/features/*",
        "../sdk/features/*"
      ],
      "@sdk/campaigns": [
        "../sdk/features/campaigns"
      ],
      "@sdk/campaigns/*": [
        "../sdk/features/campaigns/*"
      ],
      "@sdk/features/campaigns": [
        "../sdk/features/campaigns"
      ],
      "@sdk/features/campaigns/*": [
        "../sdk/features/campaigns/*"
      ],
      "@sdk/dashboard": [
        "../sdk/features/dashboard"
      ],
      "@sdk/features/dashboard": [
        "../sdk/features/dashboard"
      ],
      "@sdk/overview": [
        "../sdk/features/overview"
      ],
      "@sdk/features/overview": [
        "../sdk/features/overview"
      ],
      "@sdk/overview": [
        "../sdk/features/overview"
      ],
      "@sdk/features/overview": [
        "../sdk/features/overview"
      ],
      "@sdk/*": [
        "../sdk/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/phone-numbers/page.jsx",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
